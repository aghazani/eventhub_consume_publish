{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "react-app"
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "no-console": "warn",
    "@typescript-eslint/no-var-requires": 0
  }
}
